public class Solution {
    public string ToLowerCase(string str) {
        StringBuilder lowerCase = new StringBuilder();
        
        foreach (char c in str){
            char lowerc = (c >= 'A' && c <= 'Z') ? (char)(c +32): c;
            lowerCase.Append(lowerc);
        }
        return lowerCase.ToString();
    }
}
